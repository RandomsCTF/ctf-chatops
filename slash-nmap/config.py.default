#
# Complete these config options and rename file to config.py
#

# put your integration token here
slack_token = ''

# put a webhook URL that's configured for POST requests
slack_url = ''

# this will be the bot's username in slack channel
botname = 'nmap'

# make sure this doesn't conflict with another listener, default ports are configured to prevent that
listen_port = 5000

# number of minutes between scans allowed
cooldown_min = 2

nmap_arguments = '--top-ports 20 --open'
